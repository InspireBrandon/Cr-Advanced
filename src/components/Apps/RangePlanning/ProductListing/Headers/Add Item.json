            },
            {
                "headerName": "RSP Excl Vat",
                "field": "rsp_Excluding_VAT",
                "editable": false,
                "hide": true,
                "valueGetter": "((parseInt(data.rsp_Including_VAT) + ((parseFloat(data.rsp_Including_VAT) - parseInt(data.rsp_Including_VAT)) <= 50 ? 0.5 : 0.9)).toFixed(2) / 1.15).toFixed(2)"   
            },
            {
                "headerName": "Rounded Selling Price",
                "field": "rounded_Selling_Price",
                "editable": false,
                "hide": true,
                "valueGetter": "(parseInt(data.rsp_Including_VAT) + ((parseFloat(data.rsp_Including_VAT) - parseInt(data.rsp_Including_VAT)) <= 50 ? 0.5 : 0.9)).toFixed(2)"   
            },
            {
                "headerName": "Markup %",
                "field": "markup_Percentage",
                "editable": false,
                "hide": true,
                "valueGetter": "parseFloat((((parseInt(data.rsp_Including_VAT) + ((parseFloat(data.rsp_Including_VAT) - parseInt(data.rsp_Including_VAT)) <= 50 ? 0.5 : 0.9)).toFixed(2) / 1.15).toFixed(2) - parseFloat(data.unit_Cost).toFixed(2)) / parseFloat(data.unit_Cost).toFixed(2)).toFixed(2) * 100"   
            },
            {
                "headerName": "Gross Profit %",
                "field": "gross_Profit_Percentage",
                "editable": false,
                "hide": true,
                "valueGetter": "(parseFloat((((parseInt(data.rsp_Including_VAT) + ((parseFloat(data.rsp_Including_VAT) - parseInt(data.rsp_Including_VAT)) <= 50 ? 0.5 : 0.9)).toFixed(2) / 1.15).toFixed(2) - parseFloat(data.unit_Cost).toFixed(2)) / ((parseInt(data.rsp_Including_VAT) + ((parseFloat(data.rsp_Including_VAT) - parseInt(data.rsp_Including_VAT)) <= 50 ? 0.5 : 0.9)).toFixed(2) / 1.15).toFixed(2)) * 100).toFixed(0)"
            },
            {
                "headerName": "Category GP %",
                "field": "category_Gross_Profit_Percentage",
                "editable": true,
                "hide": true
            },
            {
                "headerName": "Shortfall %",
                "field": "shortfall_Percentage",
                "editable": true,
                "hide": true,
                "valueGetter": "data.category_Gross_Profit_Percentage - (parseFloat((((parseInt(data.rsp_Including_VAT) + ((parseFloat(data.rsp_Including_VAT) - parseInt(data.rsp_Including_VAT)) <= 50 ? 0.5 : 0.9)).toFixed(2) / 1.15).toFixed(2) - parseFloat(data.unit_Cost).toFixed(2)) / ((parseInt(data.rsp_Including_VAT) + ((parseFloat(data.rsp_Including_VAT) - parseInt(data.rsp_Including_VAT)) <= 50 ? 0.5 : 0.9)).toFixed(2) / 1.15).toFixed(2)) * 100).toFixed(0)"
            },
